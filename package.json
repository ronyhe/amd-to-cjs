{
  "name": "amd-to-cjs",
  "type": "module",
  "packageManager": "yarn@3.2.3",
  "author": "ronyhe",
  "description": "Convert AMD modules to commonJS modules",
  "homepage": "https://github.com/ronyhe/amd-to-cjs",
  "bugs": "https://github.com/wix-private/document-management/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ronyhe/amd-to-cjs"
  },
  "main": "index.mjs",
  "keywords": ["amd", "amd to common", "amd to cjs", "convert amd", "migrate amd"],
  "version": "0.0.1",
  "scripts": {
    "test": "ava test/*.spec.mjs",
    "test:debug": "ava debug",
    "lint": "eslint index.mjs src/*.mjs test/*.mjs",
    "lint:fix": "eslint index.mjs src/*.mjs test/*.mjs --fix",
    "check": "yarn lint && yarn test"
  },
  "devDependencies": {
    "ava": "^4.3.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@babel/generator": "^7.18.13",
    "@babel/parser": "^7.18.13",
    "@babel/types": "^7.18.13",
    "ramda": "^0.28.0"
  }
}
